{
    "version": 2,
    "builds": [
      {
        "src": "api/get-car-details.py",
        "use": "@vercel/python"
      },
      {
        "src": "*.html",
        "use": "@vercel/static"
      },
      {
        "src": "*.js",
        "use": "@vercel/static"
      },
      {
        "src": "*.css",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/get-car-details",
        "dest": "api/get-car-details.py"
      },
      {
        "src": "/(.*\\.(html|js|css))",
        "dest": "/$1"
      },
      {
        "src": "/(.*)",
        "dest": "index.html"
      }
    ]
  }